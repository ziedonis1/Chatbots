    public static void main(String[] args) {

        Scanner scan = new Scanner(System.in);
        Random rnd = new Random();
        int randomSkaitlis = rnd.nextInt(10);

        System.out.println("Uzmini Å¡o skaitli!");

        int skaitlis = scan.nextInt();

        if (skaitlis == randomSkaitlis) {
            System.out.println("Apsveicu!");
        } else {
            System.out.println("Varbut citreiz!");
        }
    }
}

